{
	"window.zoomLevel": -1,
	"window.commandCenter": true,
	"window.titleBarStyle": "custom",

	"editor.tabSize": 3,
	"editor.fontSize": 18,
	"editor.lineHeight": 46,
	"editor.padding.top": 20,
	"editor.padding.bottom": 20,
	"editor.formatOnSave": true,
	"editor.linkedEditing": true,
	"editor.smoothScrolling": true,
	"editor.suggestLineHeight": 35,
	"editor.inlayHints.fontSize": 12,
	"editor.renderWhitespace": "all",
	"editor.inlayHints.padding": true,
	"editor.suggestSelection": "first",
	"editor.inlineSuggest.enabled": true,
	"editor.stickyScroll.enabled": false,
	"editor.suggest.showSnippets": false,
	"editor.accessibilitySupport": "off",
	"editor.guides.bracketPairs": "active",
	"editor.cursorSmoothCaretAnimation": "on",
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.suggest.snippetsPreventQuickSuggestions": true,
	"editor.wordSeparators": "`~!@#$%^&*-()=+[{]}\\|;:'\",.<>/?",
	"editor.fontVariations": "'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'zero'",
	"editor.fontLigatures": "'ss01','ss02','ss03', 'cv01','cv02','cv10','cv03','cv06','cv09'",
	"editor.fontFamily": "\"CommitMono\",MonoLisa,\"BerkeleyMono Nerd Font\",\"Rec Mono Linear\",\"IBM Plex\"",

	"workbench.tree.indent": 16,
	"workbench.colorTheme": "Nord Deep",
	"workbench.editor.empty.hint": "hidden",
	"workbench.editor.enablePreview": false,
	"workbench.iconTheme": "material-icon-theme",
	"workbench.tree.renderIndentGuides": "always",

	"prettier.semi": false,
	"prettier.printWidth": 80,
	"prettier.singleQuote": true,
	"prettier.useTabs": true,

	"terminal.integrated.defaultProfile.osx": "zsh",
	"terminal.explorerKind": "external",
	"terminal.external.osxExec": "iTerm.app",
	"terminal.integrated.fontFamily": "CommitMono",
	"terminal.integrated.lineHeight": 1.5,
	"terminal.integrated.fontSize": 18,

	"explorer.compactFolders": false,
	"symbols.hidesExplorerArrows": false,

	"emmet.triggerExpansionOnTab": true,
	"emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
	"emmet.includeLanguages": {
		"javascript": "javascriptreact",
		"typescript": "typescriptreact",
		"postcss": "css"
	},

	"[markdown]": { "editor.wordWrapColumn": 50 },
	"[css]": { "editor.suggest.showVariables": true },
	"[html]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
	"[json]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
	"[typescript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
	"[javascriptreact]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
	"[typescriptreact]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.snippetSuggestions": "bottom"
	},

	"files.autoSaveDelay": 1500,
	"files.associations": { "*.jsx": "javascriptreact" },

	"security.workspace.trust.enabled": false,

	"tailwindCSS.experimental.classRegex": [
		["tailwind|tw.style\\(([^)]*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"]
	],
	"http.systemCertificates": false,
	"markdown-preview-enhanced.previewTheme": "solarized-dark.css",
	"git.mergeEditor": false,

	"totalTypeScript.hideAllTips": false,
	"totalTypeScript.hideBasicTips": true,
	"totalTypeScript.hiddenTips": [
		"passing-generics-to-types",
		"typeof",
		"record-utility-type",
		"type-predicate",
		"as-const",
		"generic-slots-in-functions",
		"returntype-utility-type",
		"tuple-type",
		"parameters-utility-type",
		"awaited-utility-type",
		"keyof",
		"extract-utility-type",
		"exclude-utility-type",
		"as-const-on-object",
		"mapped-type",
		"uppercase-utility-type",
		"type-alias-with-generics",
		"type-alias-with-multiple-generics",
		"conditional-type",
		"nested-conditional-type",
		"never-keyword",
		"infer",
		"in-operator-narrowing",
		"partial-utility-type",
		"omit-utility-type",
		"interface-with-generics",
		"interface-with-multiple-generics",
		"pick-utility-type",
		"non-null-expression",
		"nonnullable-utility-type",
		"readonly-utility-type"
	],

	"typescript.tsserver.log": "off",
	"typescript.updateImportsOnFileMove.enabled": "always",
	"typescript.inlayHints.parameterNames.enabled": "all",
	"javascript.inlayHints.parameterNames.enabled": "all",
	"javascript.inlayHints.enumMemberValues.enabled": true,
	"javascript.updateImportsOnFileMove.enabled": "never",

	"gitlens.views.showRelativeDateMarkers": false,
	"gitlens.ai.experimental.generateCommitMessage.enabled": false,

	"supermaven.enable": { "*": true },
	"supermaven.enableFixWithSupermaven": true,
	"diffEditor.renderSideBySide": true,
	"diffEditor.ignoreTrimWhitespace": true,

	"ReactNativeIDE.panelLocation": "tab",
	"githubPullRequests.pullBranch": "never",
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue"
}
